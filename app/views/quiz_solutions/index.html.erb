<p id="notice"><%= notice %></p>

<h1>Quiz Solutions</h1>

<table>
  <thead>
    <tr>
      <th>Quize</th>
      <th>Student</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_solutions.each do |quiz_solution| %>
      <tr>
        <td><%= quiz_solution.quize_id %></td>
        <td><%= quiz_solution.student_id %></td>
        <td><%= quiz_solution.grade %></td>
        <td><%= link_to 'Show', quiz_solution %></td>
        <td><%= link_to 'Edit', edit_quiz_solution_path(quiz_solution) %></td>
        <td><%= link_to 'Destroy', quiz_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz Solution', new_quiz_solution_path %>
