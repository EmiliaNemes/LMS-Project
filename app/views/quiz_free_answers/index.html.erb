<p id="notice"><%= notice %></p>

<h1>Quiz Answers</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_free_answers.each do |quiz_free_answer| %>
      <tr>
        <td><%= quiz_free_answer.quiz_question_id %></td>
        <td><%= quiz_free_answer.free_answer %></td>
        <td><%= link_to 'Show', quiz_free_answer %></td>
        <td><%= link_to 'Edit', edit_quiz_free_answer_path(quiz_free_answer) %></td>
        <td><%= link_to 'Destroy', quiz_free_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz Answer', new_quiz_free_answer_path %>
