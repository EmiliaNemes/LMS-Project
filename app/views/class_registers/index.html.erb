<p id="notice"><%= notice %></p>

<h1>Class Registers</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Assignment</th>
      <th>Student</th>
      <th>Grade</th>
      <th>Grading teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @class_registers.each do |class_register| %>
      <tr>
        <td><%= class_register.course_id %></td>
        <td><%= class_register.assignment_id %></td>
        <td><%= class_register.student_id %></td>
        <td><%= class_register.grade %></td>
        <td><%= class_register.grading_teacher_id %></td>
        <td><%= link_to 'Show', class_register %></td>
        <td><%= link_to 'Edit', edit_class_register_path(class_register) %></td>
        <td><%= link_to 'Destroy', class_register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class Register', new_class_register_path %>
